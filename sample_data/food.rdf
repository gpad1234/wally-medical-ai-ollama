<?xml version="1.0"?>
<rdf:RDF xmlns="http://example.org/food#"
     xml:base="http://example.org/food"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
    
    <owl:Ontology rdf:about="http://example.org/food">
        <rdfs:label>Food and Nutrition Ontology</rdfs:label>
        <rdfs:comment>Ontology describing food items, ingredients, and nutritional information</rdfs:comment>
    </owl:Ontology>
    
    <!-- Classes -->
    
    <owl:Class rdf:about="http://example.org/food#Food">
        <rdfs:label>Food</rdfs:label>
        <rdfs:comment>Any nutritious substance that people or animals eat</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/food#Fruit">
        <rdfs:label>Fruit</rdfs:label>
        <rdfs:comment>Edible plant structure containing seeds</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://example.org/food#Food"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/food#Vegetable">
        <rdfs:label>Vegetable</rdfs:label>
        <rdfs:comment>Edible plant or part of a plant</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://example.org/food#Food"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/food#Meat">
        <rdfs:label>Meat</rdfs:label>
        <rdfs:comment>Animal flesh used as food</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://example.org/food#Food"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/food#Dairy">
        <rdfs:label>Dairy</rdfs:label>
        <rdfs:comment>Food product made from milk</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://example.org/food#Food"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/food#Dish">
        <rdfs:label>Dish</rdfs:label>
        <rdfs:comment>A prepared food item or meal</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/food#Nutrient">
        <rdfs:label>Nutrient</rdfs:label>
        <rdfs:comment>A substance that provides nourishment</rdfs:comment>
    </owl:Class>
    
    <!-- Data Properties -->
    
    <owl:DatatypeProperty rdf:about="http://example.org/food#calories">
        <rdfs:label>calories</rdfs:label>
        <rdfs:comment>Caloric content per 100g</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/food#Food"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://example.org/food#protein">
        <rdfs:label>protein</rdfs:label>
        <rdfs:comment>Protein content in grams per 100g</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/food#Food"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://example.org/food#isOrganic">
        <rdfs:label>is organic</rdfs:label>
        <rdfs:comment>Whether the food is organically produced</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/food#Food"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://example.org/food#allergen">
        <rdfs:label>allergen</rdfs:label>
        <rdfs:comment>Common allergens present</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/food#Food"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <!-- Object Properties -->
    
    <owl:ObjectProperty rdf:about="http://example.org/food#contains">
        <rdfs:label>contains</rdfs:label>
        <rdfs:comment>Dish contains a food ingredient</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/food#Dish"/>
        <rdfs:range rdf:resource="http://example.org/food#Food"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/food#hasNutrient">
        <rdfs:label>has nutrient</rdfs:label>
        <rdfs:comment>Food contains this nutrient</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/food#Food"/>
        <rdfs:range rdf:resource="http://example.org/food#Nutrient"/>
    </owl:ObjectProperty>

</rdf:RDF>
