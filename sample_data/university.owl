<?xml version="1.0"?>
<rdf:RDF xmlns="http://example.org/university#"
     xml:base="http://example.org/university"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    
    <owl:Ontology rdf:about="http://example.org/university">
        <rdfs:label>University Domain Ontology</rdfs:label>
        <rdfs:comment>A sample ontology for the university domain</rdfs:comment>
    </owl:Ontology>
    
    <!-- Classes -->
    
    <owl:Class rdf:about="http://example.org/university#Person">
        <rdfs:label>Person</rdfs:label>
        <rdfs:comment>A human being</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/university#Student">
        <rdfs:label>Student</rdfs:label>
        <rdfs:comment>A university student</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://example.org/university#Person"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/university#Professor">
        <rdfs:label>Professor</rdfs:label>
        <rdfs:comment>A university professor</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://example.org/university#Person"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/university#Course">
        <rdfs:label>Course</rdfs:label>
        <rdfs:comment>An academic course</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/university#Department">
        <rdfs:label>Department</rdfs:label>
        <rdfs:comment>An academic department</rdfs:comment>
    </owl:Class>
    
    <!-- Data Properties -->
    
    <owl:DatatypeProperty rdf:about="http://example.org/university#name">
        <rdfs:label>name</rdfs:label>
        <rdfs:comment>The name of a person</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/university#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://example.org/university#email">
        <rdfs:label>email</rdfs:label>
        <rdfs:comment>Email address</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/university#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://example.org/university#studentID">
        <rdfs:label>student ID</rdfs:label>
        <rdfs:comment>Student identification number</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/university#Student"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://example.org/university#courseCode">
        <rdfs:label>course code</rdfs:label>
        <rdfs:comment>Course identification code</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/university#Course"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://example.org/university#credits">
        <rdfs:label>credits</rdfs:label>
        <rdfs:comment>Number of course credits</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/university#Course"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    
    <!-- Object Properties -->
    
    <owl:ObjectProperty rdf:about="http://example.org/university#teaches">
        <rdfs:label>teaches</rdfs:label>
        <rdfs:comment>Professor teaches a course</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/university#Professor"/>
        <rdfs:range rdf:resource="http://example.org/university#Course"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/university#enrolledIn">
        <rdfs:label>enrolled in</rdfs:label>
        <rdfs:comment>Student is enrolled in a course</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/university#Student"/>
        <rdfs:range rdf:resource="http://example.org/university#Course"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/university#belongsTo">
        <rdfs:label>belongs to</rdfs:label>
        <rdfs:comment>Person belongs to a department</rdfs:comment>
        <rdfs:domain rdf:resource="http://example.org/university#Person"/>
        <rdfs:range rdf:resource="http://example.org/university#Department"/>
    </owl:ObjectProperty>

</rdf:RDF>
